{
	"name": "discord-custom-game-bot",
	"version": "1.1.1",
	"description": "",
	"license": "MIT",
	"author": "secchanu",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"build": "tsc",
		"clean": "del-cli dist",
		"dev": "ts-node --files src/index.ts",
		"format": "run-s format:prettier format:sort-package-json",
		"format:prettier": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check --write",
		"format:sort-package-json": "sort-package-json",
		"test": "run-s test:lint test:prettier test:types",
		"test:lint": "eslint --ext .ts .",
		"test:prettier": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check",
		"test:types": "tsc -b",
		"prepare": "husky install"
	},
	"dependencies": {
		"discord.js": "^14.9.0",
		"dotenv": "^16.0.3",
		"keyv": "^4.5.2",
		"keyv-file": "^0.2.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"npm-run-all": "^4.1.5",
		"prettier": "2.8.7",
		"sort-package-json": "^2.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}
